<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="@android:style/Theme.NoTitleBar.Fullscreen">
    </style>

    <style name="popupWindow" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pop_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_exit</item>
    </style>
    <style name="popupWindow_sec" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pop_enter_sec</item>
        <item name="android:windowExitAnimation">@anim/pop_exit_sec</item>
    </style>
    <style name="popupWindow_sec_" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pop_enter_sec_</item>
        <item name="android:windowExitAnimation">@anim/pop_exit_sec_</item>
    </style>
    <style name="seekBar">
        <item name="android:paddingLeft">7dp</item>
        <item name="android:paddingBottom">17dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">17dp</item>
    </style>
    <style name="settingDialog_style" parent="@style/Base.V21.Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@drawable/shape_window</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:buttonBarStyle">@null</item>

    </style>
</resources>
